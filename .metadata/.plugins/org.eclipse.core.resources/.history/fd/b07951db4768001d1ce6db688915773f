package br.edu.infnet.pedido.modelo.entidades;


public class Hospede {

	
	@Override
	public String toString() {
		return "Cliente [codigo=" + codigo + ", nome=" + nome + "]";
	}

	private Long codigo;

	private String nome;

	public Hospede() {
	}

	public Hospede(String nome) {
		super();
		this.nome = nome;
	}

	public Hospede(String nome, Long codigo) {
		this.nome = nome;
		this.codigo = codigo;
	}

	public Long getCodigo() {
		return codigo;
	}

	public String getNome() {
		return nome;
	}

}
